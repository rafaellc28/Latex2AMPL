var x{1..4} >= 0;


minimize obj: x[1] - x[2] - x[3] - x[1] * x[3] + x[1] * x[4] + x[2] * x[3] - x[2] * x[4];

s.t. C1 : 8 - x[1] - 2 * x[2] >= 0;

s.t. C2 : 12 - 4 * x[1] - x[2] >= 0;

s.t. C3 : 12 - 3 * x[1] - 4 * x[2] >= 0;

s.t. C4 : 8 - 2 * x[3] - x[4] >= 0;

s.t. C5 : 8 - x[3] - 2 * x[4] >= 0;

s.t. C6 : 5 - x[3] - x[4] >= 0;


